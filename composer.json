{
    "name": "nadialabs/nadia-rad-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "NadiaRadBundle provides tools for developing Symfony application easily and rapidly.",
    "require": {
        "php": ">=8.1",
        "symfony/dependency-injection": "^6.2"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Nadia\\Bundle\\NadiaRadBundle\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Nadia\\Bundle\\NadiaRadBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "LeoOnTheEarth",
            "email": "leo.on.the.earth@gmail.com"
        }
    ]
}
